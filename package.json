{
  "name": "legal-process-fetcher",
  "version": "1.0.0",
  "description": "Comprehensive Brazilian legal process fetcher using CNJ DataJud API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "search": "node main.js",
    "test": "node test.js"
  },
  "bin": {
    "legal-search": "./main.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "legal",
    "process",
    "cnj",
    "datajud",
    "brazilian-courts",
    "cnpj",
    "web-app"
  ],
  "author": "Legal Process Fetcher",
  "license": "MIT"
}